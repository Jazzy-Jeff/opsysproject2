#include "process.h"

Process::Process() {
  id = "";
  size = 0;
  arrivalTime = 0;
  burstTime = 0;
  remainingBurstTime = 0;
}

Process::Process(string id1, int size1, int arrivalTime1, int burstTime1){
  id = id1;
  size = size1;
  arrivalTime = arrivalTime;
  burstTime = burstTime1;
  remainingBurstTime = burstTime1;
}

string Process::getId() const{
  return id;
}

int Process::getSize() const {
  return size;
}

int Process::getArrivalTime() const{
  return arrivalTime;
}

int Process::getBurstTime() const{
  return burstTime;
}

int Process::getRemainingBurstTime() const{
  return remainingBurstTime;
}

